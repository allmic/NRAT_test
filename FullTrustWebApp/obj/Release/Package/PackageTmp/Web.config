<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="NewRelic.AppName" value="FullTrustWebApp" />
    <add key="NewRelic.AgentEnabled" value="true" />
    <add key="NewRelicAgentSetupTransactionEnable" value="1" />
  </appSettings>
  <system.web>
    <compilation targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />

    <trust level="Full" />
  </system.web>
</configuration>
